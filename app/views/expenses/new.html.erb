<%= form_for(@expense) do |form| %>
    <%= form.text_field :title%>


    <%= form.fields_for :expense_category,  @expense.expense_category do |category_form| %>
        <%= category_form.label :title %>
          <%= category_form.text_field :title %>
        <% end %>

      <%= form.collection_select :user_ids, User.all, :id, :name , {}, multiple: true%>
    
    <%= form.submit %>
<% end %>